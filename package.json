{
  "name": "next-props-shared",
  "version": "0.1.24",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "cd example && npm run dev",
    "clean": "rm -rf dist && rm -rf example/node_modules && rm -rf example/.next",
    "copy-assets": "copyfiles -u 2 src/nps/assets/**/* dist/assets",
    "build:ts": "tsc --emitDeclarationOnly && npm run copy-assets",
    "build:js": "rollup -c",
    "build": "npm run clean && npm run build:ts && npm run build:js",
    "deploy:check": "npm pack --dry-run",
    "deploy": "npm run build && npm publish --access public"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "raw-loader": "^4.0.2",
    "rollup": "^4.40.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svg-import": "^3.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx-source": "^7.25.9",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "react-draggable": "^4.4.6"
  }
}
